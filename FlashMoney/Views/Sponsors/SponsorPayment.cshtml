<script type="text/javascript">
    debugger
    function payWithPaystackz2() {
        debugger
        var amt2 = $('#amount2').val();
        var email2 = $('#Email2').val();
        //var city = $('#validationCustom03').val();
        var phoneNum2 = $('#phonenumber2').val();

        debugger
        if (amt2 == "") {
            alert("Please select ...")
        } else {
            var _email2 = email2;
            var _phoneNum2 = phoneNum2;
            //var _state = city;
            var amount2 = (parseInt(amt2)) * 100
            //alert(amount);
            var _amt2 = amount2;
            console.log("amt:" + _amt2);

            var handler = PaystackPop.setup({
