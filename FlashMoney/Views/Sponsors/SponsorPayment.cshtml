                ref: '' + Math.floor((Math.random() * 1000000000) + 1), // generates a pseudo-unique reference. Please replace with a reference you generated. Or remove the line entirely so our API will generate one for you
                metadata: {
                    custom_fields: [
                        {
                            display_name: _email2,
                            variable_name: "mobile_number",
                            value: "+2348066778135"
                        }
                    ]
                },
                callback: function (response) {

                    var responseData = response.reference;
                    /**/
                    window.location.href = '/Student/ScholarshipPayConfirm?reference=' + responseData + '&Email=' + _email2 + '&phone=' + _phoneNum2;
                    /**/
                },
                onClose: function () {
                    alert('window closed');
                }
            });
            handler.openIframe();
        }
    }

</script>
